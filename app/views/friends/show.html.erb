<div class="jumbotron">
  <h1 class="display-4">
  <%= @friend.first_name %>
  <%= @friend.last_name %>
  </h1>
  <p class="lead">
  Email: <%= @friend.email %> |
  Phone: <%= @friend.phone %> |
  Twitter: <%= @friend.twitter %>
  </p>
  <hr class="my-4">
  <%= link_to "Edit", edit_friend_path(@friend), class:"btn btn-outline-info"  %> 
  <%= link_to "Back to friends", friends_path, class:"btn btn-dark"  %>
  <br> </br>
</div>